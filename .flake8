[flake8]
ignore = D203,D100,D101,D400,D401,D106,W503,F403,F405,D403,D104,B008,F722,N805,N818,N815,N803,I201,E203, A003, D103, C901, D202,E302
extend-ignore = E203, A002, D107, D102, E712, E711, SIM907, C901
exclude =
    .git,
    .venv,
    __pycache__,
    alembic,
    .pytest_cache,
    __pycache__,
filename =
    *.py
max-complexity = 12
import-order-style = google
application-import-names =
    main,
    handler,
max-line-length = 120
max-linenumber = 500
accept-encodings = utf-8,utf-16
inline-quotes = double
multiline-quotes = double

[pytest]
env =
    ENV=test
